package unidad1prog3;
import java.util.Scanner;

public class ejerciciodeordenacion {
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner lector = new Scanner(System.in);
		System.out.println("ingrese 3 valores diferentes : ");
		
		System.out.println("numero 1; ");
		int num1 = lector.nextInt();
		
		System.out.println("numero 2; ");
		int num2 = lector.nextInt();
		
		System.out.println("numero 3; ");
		int num3 = lector.nextInt();
		
		if(num1>num2)
		{
			if(num1>num3)
			{
				if(num2>num3)
				{
					System.out.println("el numero " + num1 + "es mayor a " + num2 + "que este a su vez es mayor que el " + num3);
				}else
				{
					System.out.println("el numero " + num1 + "es mayor a " + num3 + "que este a su vez es mayor que el " + num2);
				}
			}else
			{
				System.out.println("el numero " + num3 + "es mayor a " + num1 + "que este a su vez es mayor que el " + num2);
			}
		}else//num2>num1
		{
			if(num2>num3)
			{
				if (num1>num3)
				{
					System.out.println("el numero " + num2 + "es mayor a " + num1 + "que este a su vez es mayor que el " + num3);
				}else//num3 es mayor a num1
				{
					System.out.println("el numero " + num2 + "es mayor a " + num3 + "que este a su vez es mayor que el " + num1);
				}
			}else
			{
				System.out.println("el numero " + num3 + "es mayor a " + num2 + "que este a su vez es mayor que el " + num1);
			}
		}

	}
	

}
